node{

    stage('Checkout') {
        checkout scm
    }

    stage('Release to QA'){
        sh "kubectl config --kubeconfig=/etc/.kube/config use-context aws_QA"
        sh "kubectl replace --kubeconfig=/etc/.kube/config  -f turbine-server-parent/turbine-server/kubernetes/turbine-server-qa-deployment.yml --record"
    }
}